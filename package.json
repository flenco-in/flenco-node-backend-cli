{
  "name": "flenco-node-backend-cli",
  "version": "0.1.1",
  "description": "CLI tool to generate complete backend projects with Express, TypeScript, and Prisma",
  "main": "dist/index.js",
  "bin": {
    "flenco-node-backend-cli": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prepublishOnly": "npm run build",
    "test": "echo \"No test specified\" && exit 0"
  },
  "keywords": [
    "express",
    "backend",
    "generator",
    "cli",
    "typescript",
    "prisma",
    "crud",
    "api"
  ],
  "author": "Atish Paul",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flenco-in/flenco-node-backend-cli.git"
  },
  "bugs": {
    "url": "https://github.com/flenco-in/flenco-node-backend-cli/issues"
  },
  "homepage": "https://github.com/flenco-in/flenco-node-backend-cli#readme",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "zod": "^3.22.4",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.2.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "prisma": "^5.8.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
